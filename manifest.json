{
    "name": "Cashout manual",
    "version": "0.1",
    "description": "Plugin para ficar verificando as odd's da betway/rivalry para cashout manual",
    "permissions": ["activeTab", "declarativeContent", "storage"],
    "background": {
      "scripts": ["/scripts/background.js"],
      "persistent": false
    },
    "page_action": {
      "default_popup": "/views/popup.html",
      "default_icon": {
        "16": "/resources/images/get_started16.png",
        "32": "/resources/images/get_started32.png",
        "48": "/resources/images/get_started48.png",
        "128": "/resources/images/get_started128.png"
      }
    },
    "icons": {
      "16": "/resources/images/get_started16.png",
      "32": "/resources/images/get_started32.png",
      "48": "/resources/images/get_started48.png",
      "128": "/resources/images/get_started128.png"
    },
    "manifest_version": 2
  }